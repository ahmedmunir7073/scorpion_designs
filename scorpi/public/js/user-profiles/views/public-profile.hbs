 {{> messages }}

{{#if username }}
<h2>{{ username }}'s profile</h2>
<table class="table">
    <tr>
        <td>Name: </td>
        <td>{{ name }}</td>
    </tr>
    <tr>
        <td>Phone:</td>
        <td>{{ phone }}</td>
    </tr>
    <tr>
        <td>Address: </td>
        <td>{{ address }}</td>
    </tr>
    <tr>
        <td>Bio: </td>
        <td>{{ bio }}</td>
    </tr>
    <tr>
        <td>Company: </td>
        <td>{{ company }}</td>
    </tr>
    <tr>
        <td>City: </td>
        <td>{{ city }}</td>
    </tr>
    <tr>
        <td>State: </td>
        <td>{{ state }}</td>
    </tr>
    <tr>
        <td>Country: </td>
        <td>{{ country }}</td>
    </tr>
</table>

{{/if}}




